{
  "name": "@capgo/native-audio",
  "version": "5.1.8",
  "description": "A native plugin for native audio engine",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "CapgoNativeAudio.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "audio",
    "media",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --autocorrect --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier --config .prettierrc.js \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api NativeAudio --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist && cd android && ./gradlew clean && cd ..",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "author": "Martin Donadieu <martindonadieu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^4.7.3",
    "@capacitor/cli": "^4.7.3",
    "@capacitor/core": "^4.7.3",
    "@capacitor/docgen": "^0.2.1",
    "@capacitor/ios": "^4.7.3",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^2.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "prettier-plugin-java": "^2.1.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.20.2",
    "swiftlint": "^1.0.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@capacitor/core": "^4.0.0"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cap-go/native-audio"
  },
  "bugs": {
    "url": "https://github.com/Cap-go/native-audio/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
